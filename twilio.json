                }
            },
            "32bit": {
                "url": "https://twilio-cli-prod.s3.amazonaws.com/twilio-win32-x86.tar.xz",
                "hash": {
                    "url": "https://twilio-cli-prod.s3.amazonaws.com/win32-x86",
                    "jsonpath": "$.sha256xz"
                }
            }
        }
    },
    "post_install": "& $dir\\twilio\\bin\\node.exe $dir\\twilio\\welcome.js"
